{
  "manifest_version": 3,
  "name": "Dynamic Bookmarks",
  "version": "1.1",
  "description": "Dynamically updates bookmarks based on shared configuration.",
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
}
}